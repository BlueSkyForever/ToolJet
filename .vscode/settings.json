{
    "[javascript, typescript]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "eslint.format.enable": true,
    "editor.formatOnSave": true,
    "json.schemas": [
        {
            "fileMatch": [
                "/*.operations.json"
            ],
            "url": "https://jsonkeeper.com/b/0XIY"
        },
        {
            "fileMatch": [
                "/*.manifest.json"
            ],
            "url": "https://jsonkeeper.com/b/0XIY"
        }
    ]
}