{
  "$schema": "https://raw.githubusercontent.com/ToolJet/ToolJet/develop/plugins/schemas/manifest.schema.json",
  "title": "Mindsdb datasource",
  "description": "A schema defining Mindsdb datasource",
  "type": "database",
  "source": {
    "name": "Mindsdb",
    "kind": "mindsdb",
    "exposedVariables": {
      "isLoading": false,
      "data": {},
      "rawData": {}
    },
    "options": {
      "url": {
        "type": "string"
      },
      "apikey": {
        "type": "string"
      }
    }
  },
  "defaults": {
    "url": {
      "value": "http://localhost:47334/api"
    }
  },
  "properties": {
    "url": {
      "label": "URL",
      "key": "url",
      "type": "text",
      "description": "Enter URL"
    },
    "apikey": {
      "label": "Api Key",
      "key": "apikey",
      "type": "password",
      "description": "Enter Api Key"
    }
  },
  "required": ["url"]
}
